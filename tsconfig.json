{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ES2020"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": [
      "node"
    ],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "outDir": "dist",
    "rootDir": "src",
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@router": [
        "./src/router/index.ts"
      ]
    }
  },
  "include": [
    "src"
  ]
}